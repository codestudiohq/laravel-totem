{
  "name": "studio/laravel-totem",
  "description": "A Laravel package to manage your cron jobs through a beautiful dashboard",
  "keywords": [
    "laravel",
    "schedule",
    "manager",
    "Code Studio",
    "totem"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roshan Gautam",
      "email": "roshan.gautam@hotmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/bus":"~5.4.0",
    "illuminate/console": "~5.4.0",
    "illuminate/contracts": "~5.4.0",
    "illuminate/database":"~5.4.0",
    "illuminate/events": "~5.4.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "orchestra/database": "~3.0",
    "orchestra/testbench" : "~3.0",
    "phpunit/phpunit": "~6.0"
  },
  "suggest": {
    "nexmo/client": "Required for sms notifications."
  },
  "autoload": {
    "psr-4": {
      "Studio\\Totem\\": "src/",
      "Studio\\Totem\\Tests\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Studio\\Totem\\Providers\\TotemServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
