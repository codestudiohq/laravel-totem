{
  "name": "studio/laravel-totem",
  "description": "A Laravel package to manage your cron jobs through a beautiful dashboard",
  "keywords": [
    "laravel",
    "schedule",
    "manager",
    "Code Studio",
    "totem"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roshan Gautam",
      "email": "roshan.gautam@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/bus": "~5.8.0",
    "illuminate/console": "~5.8.0",
    "illuminate/contracts": "~5.8.0",
    "illuminate/database": "~5.8.0",
    "illuminate/events": "~5.8.0",
    "illuminate/notifications": "~5.8.0",
    "laravelcollective/html": "^5.8"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/database": "^3.7",
    "orchestra/testbench" : "^3.8",
    "phpunit/phpunit": "^7.5"
  },
  "suggest": {
    "nexmo/client": "Required for sms notifications."
  },
  "autoload": {
    "psr-4": {
      "Studio\\Totem\\": "src/",
      "Studio\\Totem\\Tests\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-3.0": "3.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Studio\\Totem\\Providers\\TotemServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
