{
  "name": "studio/laravel-totem",
  "description": "A Laravel package to manage your cron jobs through a beautiful dashboard",
  "keywords": [
    "laravel",
    "schedule",
    "manager",
    "Code Studio",
    "totem"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roshan Gautam",
      "email": "roshan.gautam@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.0.0|^8.1.0",
    "ext-json": "*",
    "doctrine/dbal": "^3.1",
    "illuminate/bus": "^10.0",
    "illuminate/console": "^10.0",
    "illuminate/contracts": "^10.0",
    "illuminate/database": "^10.0",
    "illuminate/events": "^10.0",
    "illuminate/notifications": "^10.0",
    "laravelcollective/html": "^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench" : "^8.0",
    "phpunit/phpunit": "^9.0"
  },
  "suggest": {
    "nexmo/client": "Required for sms notifications."
  },
  "autoload": {
    "psr-4": {
      "Studio\\Totem\\": "src/",
      "Studio\\Totem\\Tests\\": "tests/",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-10.0": "10.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Studio\\Totem\\Providers\\TotemServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
